{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "New Supplier Request",
    "description": "Request to create a new Supplier",
    "type": "object",
    "properties": {
      "name": {
        "description": "name of the new Supplier",
        "type": "string"
      },
      "userId": {
        "description": "user that sent the request",
        "type": "integer",
        "exclusiveMinimum": 0
      }
    },
    "required": [
      "name",
      "userId"
    ]
  }