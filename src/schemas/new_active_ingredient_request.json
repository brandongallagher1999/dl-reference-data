{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "New Active Ingredient Request",
    "description": "Request to create active ingredient",
    "type": "object",
    "properties": {
        "name": {
            "description": "name of the new active ingredient",
            "type": "string"
        },
        "userId": {
            "description": "id of the user making the request",
            "type": "number",
            "exclusiveMinimum": 0
        }
    },
    "required": [
        "name",
        "userId"
    ]
}