{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "New Tag Request",
  "description": "Request to create a new tag",
  "type": "object",
  "properties": {
    "name": {
      "description": "name of the new tag",
      "type": "string"
    },
    "userId": {
      "description": "user that sent the new tag request",
      "type": "integer",
      "exclusiveMinimum": 0
    }
  },
  "required": [
    "name",
    "userId"
  ]
}